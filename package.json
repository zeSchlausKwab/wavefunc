{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "relay": "cd relay && go run . --port 3334",
    "relay:reset": "./scripts/kill-relay.sh && cd relay && go run . --reset-all",
    "relay:kill": "./scripts/kill-relay.sh",
    "contextvm": "bun run contextvm/server.ts",
    "dev": "./scripts/dev-clean.sh",
    "dev:old": "bun run relay & sleep 1 && bun run migrate && bun run contextvm & bun --hot src/index.tsx",
    "dev:fake": "bun run relay & sleep 1 && bun run scripts/seed.ts && bun run contextvm & bun --hot src/index.tsx",
    "dev:no-seed": "bun run relay & sleep 1 && bun run contextvm & bun --hot src/index.tsx",
    "dev:frontend": "bun --hot src/index.tsx",
    "seed": "bun run scripts/seed.ts",
    "migrate": "bun run scripts/migrate_legacy.ts",
    "migrate:remote": "bun run scripts/migrate_legacy.ts 50000 --relay=wss://relay.wavefunc.live",
    "migrate:vps": "bun run scripts/trigger_remote_migration.ts",
    "migrate:vps:reset": "bun run scripts/trigger_remote_migration.ts --reset",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run build.ts",
    "build:production": "./scripts/build-production.sh",
    "setup:vps": "./scripts/setup-vps.sh",
    "deploy": "./scripts/deploy.sh",
    "routes:generate": "bunx @tanstack/router-cli generate",
    "routes:watch": "bunx @tanstack/router-cli watch",
    "tauri": "tauri",
    "tauri:dev": "bun run relay & sleep 1 && bun run migrate && bun run contextvm & tauri dev",
    "tauri:build": "tauri build",
    "tauri:android": "tauri android init && tauri android dev",
    "tauri:android:build": "tauri android build"
  },
  "dependencies": {
    "@cashu/cashu-ts": "2.7.4",
    "@cashu/crypto": "^0.3.4",
    "@contextvm/sdk": "^0.1.41",
    "@faker-js/faker": "^10.1.0",
    "@formkit/auto-animate": "^0.9.0",
    "@modelcontextprotocol/sdk": "^1.21.0",
    "@music-metadata/icy": "^0.2.1",
    "@nostr-dev-kit/blossom": "^7.0.0",
    "@nostr-dev-kit/messages": "^1.0.0",
    "@nostr-dev-kit/ndk": "^2.18.1",
    "@nostr-dev-kit/ndk-cache-dexie": "2.6.44",
    "@nostr-dev-kit/react": "^1.3.12",
    "@nostr-dev-kit/sync": "^0.3.6",
    "@nostr-dev-kit/wallet": "^0.8.10",
    "@nostr-dev-kit/wot": "^0.3.6",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-form": "^1.25.0",
    "@tanstack/react-router": "^1.136.1",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-os": "~2",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "@yudiel/react-qr-scanner": "^2.4.1",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "hls.js": "^1.6.14",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.561.0",
    "nostr-tools": "^2.17.2",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-use": "^17.6.0",
    "react18-json-view": "^0.2.9",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "webln": "^0.3.2",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.4",
    "@types/bun": "^1.3.2",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3"
  },
  "overrides": {
    "@modelcontextprotocol/sdk": "npm:@socotra/modelcontextprotocol-sdk"
  }
}
